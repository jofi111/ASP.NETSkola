@model Student
<form asp-controller="FileUpload" asp-action="Upload" enctype="multipart/form-data">
    <div class="mt-2">
    <input type="file" name="file"/>
    <input type="submit" value="Upload it!" />
    </div>
</form>
<form asp-action="Create">
    <div class="form-group mt-2">
    <label asp-for="@Model.FirstName"></label>
        <input type="text" asp-for="@Model.FirstName" class="form-control" />
    </div>
    <div class="form-group mt-2">
    <label asp-for="@Model.LastName"></label>
        <input type="text" asp-for="@Model.LastName" class="form-control" />
    </div>
    <div class="form-group mt-2">
    <label asp-for="@Model.DateOfBirth"></label>
        <input type="date" asp-for="@Model.DateOfBirth" class="form-control" />
    </div>
    <input class="btn btn-outline-success mt-2" type="submit" value="Create"/>
</form>